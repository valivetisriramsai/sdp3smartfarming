  //formats date from YYYY-MM-DD to MM/DD/YYYY
  function formatDateProper(format_date) {
    if(format_date) {
      var a = format_date.split("-");
      return (a[1] + "/" + a[2] + "/" + a[0]);
    }

    return format_date;
  };

